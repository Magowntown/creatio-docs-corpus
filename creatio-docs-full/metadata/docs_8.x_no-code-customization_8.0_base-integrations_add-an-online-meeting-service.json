{
  "url": "https://academy.creatio.com/docs/8.x/no-code-customization/8.0/base-integrations/add-an-online-meeting-service",
  "title": "Add a service to connect to online meetings",
  "breadcrumbs": [
    {
      "text": "Base integrations",
      "href": "/docs/8.x/no-code-customization/8.0/category/base-integrations"
