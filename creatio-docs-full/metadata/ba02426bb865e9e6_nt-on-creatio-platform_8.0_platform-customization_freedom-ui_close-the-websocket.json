      "code": "handlers: /**SCHEMA_HANDLERS*/[    {        request: \"crt.HandleViewModelPauseRequest\",        /* Custom implementation of a system query handler. */        handler: async (request, next) => {            /* Close the SomeWebSocket WebSocket */            (await request.$context.SomeWebSocket).close();            /* Call the next handler if it exists and return its result. */            return next?.handle(request);        }    }]/**SCHEMA_HANDLERS*/,",
